{
  "name": "DOCS-MSERRANO",
  "version": "1.0.0",
  "description": "mserrano.net documentation",
  "main": "index.js",
  "repository": "https://github.com/mserrano-dev/DOCS-MSERRANO.git",
  "author": "ENTslug",
  "license": "Apache-2.0",
  "dependencies": {
    "angular": "1.6.10",
    "angular-animate": "1.6.10",
    "angular-aria": "1.6.10",
    "angular-material": "1.1.9",
    "angular-mocks": "1.6.10",
    "angular-route": "1.6.10",
    "angular-sanitize": "1.6.10",
    "angular-translate": "2.17.1"
  },
  "scripts": {
    "preinstall": "(yarn global list | grep -q 'webpack') && : || yarn global add webpack",
    "dev": "webpack --mode development --watch",
    "prod": "webpack --mode production",
    "test": "yarn karma start frontend/tests/coverage.conf.js && php unittests/test_coverage.php unittests/ --silent && node frontend/tests/sass-true.conf.js && google-chrome tmp/index.html",
    "js:cov": "yarn karma start frontend/tests/coverage.conf.js && google-chrome tmp/js-coverage-report/index.html",
    "js": "yarn karma start frontend/tests/karma.conf.js",
    "php:cov": "php unittests/test_coverage.php unittests/",
    "php:cov:s": "php unittests/test_coverage.php unittests/ --silent",
    "php": "yarn watch 'php unittests/test_runner.php unittests/'",
    "css": "yarn watch 'node frontend/tests/sass-true.conf.js'",
    "?": "webpack --verbose"
  },
  "devDependencies": {
    "angular-templatecache-loader": "0.2.0",
    "css-loader": "0.28.11",
    "describe": "1.2.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "html-loader": "0.5.5",
    "it": "1.1.1",
    "jasmine": "2.9.0",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.1",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "3.0.0",
    "ng-annotate-loader": "0.6.1",
    "node-sass": "4.8.3",
    "sass-loader": "7.0.1",
    "sass-true": "4.0.0",
    "style-loader": "0.20.3",
    "watch": "1.0.2",
    "webpack": "4.7.0",
    "webpack-cli": "2.0.14"
  }
}
